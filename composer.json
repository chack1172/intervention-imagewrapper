{
    "name": "chack1172/intervention-imagewrapper",
    "description": "Wrapper for intervention/image to use chack1172/intervention-imagecache",
    "homepage": "https://image.intervention.io",
    "keywords": [
        "image",
        "gd",
        "imagick",
        "laravel",
        "cache"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Nereo Berardozzi",
            "email": "chack1172@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "intervention/image": "^2.7"
    },
    "require-dev": {
        "laravel/framework": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Chack1172\\Intervention\\Image\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chack1172\\Intervention\\Image\\ImageServiceProvider"
            ],
            "dont-discover": [
                "intervention/image"
            ]
        }
    },
    "minimum-stability": "stable"
}
